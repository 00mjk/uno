<Page x:Class="UITests.Shared.Windows_UI_Xaml_Controls.Frame.Page6"
	  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	  xmlns:local="using:UITests.Shared.Windows_UI_Xaml_Controls.Frame"
	  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	  mc:Ignorable="">

	<Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

		<CommandBar x:Name="MyCommandBar"
					Visibility="Collapsed"
					Content="Page6"
					Background="Red" />

		<ToggleButton Height="100"
					  HorizontalAlignment="Center"
					  VerticalAlignment="Center"
					  Content="ToggleCommandBarVisibility"
					  Click="ToggleCommandBarVisibility" />
	</Grid>
</Page>
