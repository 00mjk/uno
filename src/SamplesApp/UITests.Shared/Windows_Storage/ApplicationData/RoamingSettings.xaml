<Page
    x:Class="UITests.Shared.Windows_Storage_ApplicationData.RoamingSettings"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

	<StackPanel>
		<TextBlock x:Name="ContainerName" />
		<Button x:Name="ClearButton" Tapped="Clear" Content="Clear" />
		<Button x:Name="AddButton" Tapped="Add" Content="Add" />
		<Button x:Name="ContainsButton" Tapped="Contains" Content="Contains" />
		<Button x:Name="RemoveButton" Tapped="Remove" Content="Remove" />
		<TextBlock x:Name="Output" />
	</StackPanel>

</Page>
